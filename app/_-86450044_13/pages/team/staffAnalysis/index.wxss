.dot {
    width: 10rpx;
    height: 10rpx;
    text-align: center;
    line-height: 10rpx;
    display: inline-block;
    background: #D6D6D6;
    border-radius: 50%;
    margin-top: 20rpx;
    margin-left: 7rpx;
}

.dot.current {
    width: 20rpx;
    height: 20rpx;
    background: #E51C23;
    margin-left: -6rpx;
    border: 10rpx solid #f7babd;
    margin-top: 5rpx;
}

.add-btn {
    background: #E51C23;
    color: #fff;
}

.line {
    border-left: 1rpx solid #D6D6D6;
    position: absolute;
    height: 100%;
    top: 35rpx;
    left: 93rpx;
    display: inline-block;
}

.time-box {
    display: inline-block;
    width: 58rpx;
    margin-right: 25rpx;
    text-align: center;
}

.content {
    padding-left: 120rpx;
    min-height: 60rpx;
}

.detail-btn {
    width: 100%;
    border-radius: 0;
}

.detail-text {
    width: 400rpx;
}

.canvas-wrap {
    top: -20px;
    position: absolute;
    right: 0rpx;
}

.canvas2 {
    position: absolute;
    left: -5%;
}

.data-list {
    height: 80rpx;
    overflow-x: scroll;
    white-space: nowrap;
}

.ana-box {
    width: 28%;
}

.pro-wrap2 {
    width: 300rpx;
}

.data-text {
    width: 100%;
    top: 35rpx;
    left: 2%;
}

::-webkit-scrollbar {
    width: 0;
    height: 0;
    color: transparent;
}